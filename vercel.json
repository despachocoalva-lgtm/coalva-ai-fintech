{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    { "source": "/aviso-de-privacidad", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
